@import './css/bootstrap.min.css';

@font-face
    font-family: 'play';
    src url('fonts/playfairDisplay-Regular.ttf');

    @font-face
    font-family: 'ubuntu';
    src url('fonts/Ubuntu-Regular.ttf');

*
    font-family: 'play';

#menu-principal
    background-color: #FFF!important;
    display: flex;
    padding-top: 50px;
    font-family: 'play';

#navbarNav
    justify-content: flex-end;

#navbarNav 
    >ul 
        >li
            margin-right: 40px;  

#pie-pagina
    display: flex;
    padding: 40px;  
    background-color: #F3F3F3;
    width: 100%;
    flex-wrap: wrap;


#enlace-pie-pagina
    justify-content: flex-start;
    width: 50%;

#enlace-pie-pagina
    >ul
        list-style: none;
        display: flex;
        margin-bottom: 0;

#enlace-pie-pagina
    >ul 
        >li 
            float: left;
            margin-right: 15px;

#enlace-pie-pagina
    >ul 
        >li
            >a 
                color: #000000;

#enlace-redes-sociales
    justify-content: flex-end;
    width: 40%;
    display: flex;
    margin-left: 10%;

#enlace-redes-sociales
    >ul
        list-style: none;
        display: flex;
        margin-bottom: 0;

#enlace-redes-sociales
    >ul
        >li
            list-style: none;
            display: flex;
            float: left;
            margin-right: 15px;
 
#enlace-redes-sociales
    >ul
        >li
            >a 
                color: #575757;

